    "def euclidean_distance(v1,v2):\n",
    "    sum = 0\n",
    "    for x,y in zip(v1,v2):\n",
    "        sum += (x-y)**2\n",
    "    return sum**0.5"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 15,
   "metadata": {},
   "outputs": [],
   "source": [
    "def nearest20(training_tag,test_tags):\n",
    "    nearest_test_neighbors = []\n",
    "    for t in test_tags:\n",
    "        nearest_test_neighbors.append((t[0],euclidean_distance(training_tag,t[1])))\n",
    "    nearest_test_neighbors.sort(key=lambda x: x[1])\n",
    "    return nearest_test_neighbors[:20]"
   ]
  },
